extends layout

block content
	body.sidebar-mini.skin-purple-light
		.wrapper

			include header

			include sidebar

			// Content Wrapper. Contains page content
			.content-wrapper
				// Content Header (Page header)
				section.content-header

				// Main content
				section.content
					// Your Page Content Here
					$('.content').append('<div class="box box-body"><ul class="products-list product-list-in-box"><div class="product-info"><ul><li id=goodName></li><li>'
							+goodPrice+'원</li></ul></div></ul></div>')

						$(data).find('item').each(function(){
				var goodName = $(this).find('goodName').text();
				$('.content').append('<div class="box box-body"><ul class="products-list product-list-in-box"><div class="product-info"><ul><li>'
							+'이름 : '+ goodName+'</li></ul></div></ul></div>')
			});
			//Control Sidebar
			aside.control-sidebar.control-sidebar-light
				//Tab panes
				.tab-content
					//Home tab content
					.tab-pane.active#control-sidebar-home-tab
						h3.control-sidebar-heading
							| Recent Activity
			.control-sidebar-bg
		script(src="javascripts/goods.js")