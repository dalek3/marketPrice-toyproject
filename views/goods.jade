extends layout

block content
	body.sidebar-mini.skin-purple-light
		.wrapper

			include header

			include sidebar

			// Content Wrapper. Contains page content
			.content-wrapper
				// Content Header (Page header)
				section.content-header

				// Main content
				section.content
					// Your Page Content Here
					each item in row
						.box.box-body
							ul.products-list.product-list-in-box
								.product-info
									ul
										li
											h4
												b
													a(href='#') #{item.goodName}
										li 현재가격 : #{item.goodPrice} 원
										li 업체명 : #{item.entpName}
										li 상세정보 : #{item.detailMean}
			//Control Sidebar
			aside.control-sidebar.control-sidebar-light
				//Tab panes
				.tab-content
					//Home tab content
					.tab-pane.active#control-sidebar-home-tab
						h3.control-sidebar-heading
							| 장바구니 내역
			.control-sidebar-bg