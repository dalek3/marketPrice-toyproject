extends layout

block content
	body.sidebar-mini.skin-purple-light
		.wrapper

			include header

			include sidebar

			// Content Wrapper. Contains page content
			.content-wrapper
				// Content Header (Page header)
				section.content-header

				// Main content
				section.content
					// Your Page Content Here
					each item in row
						.box.box-body
							ul.products-list.product-list-in-box
								.product-info
									ul
										li #{item.goodName}
										li #{item.goodPrice} ì›
			//Control Sidebar
			aside.control-sidebar.control-sidebar-light
				//Tab panes
				.tab-content
					//Home tab content
					.tab-pane.active#control-sidebar-home-tab
						h3.control-sidebar-heading
							| Recent Activity
			.control-sidebar-bg